function birthdayCakeCandles(candles) {
    // Write your code here
    var count=0;
    var instances;
    for(var i=0; i<candles.length; i++){
        if(count<candles[i]){
            count=candles[i];
            instances=1;
        }
        if(count==candles[i]){
            instances++;
        }
    }
    return(instances-1);
}
